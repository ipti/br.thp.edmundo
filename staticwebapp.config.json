{
    "responseOverrides": {
      "400": {
        "redirect": "/"
      },
      "401": {
        "statusCode": 302,
        "redirect": "/"
      },
      "404": {
        "redirect": "/"
      }
    },
    "navigationFallback": {

      "rewrite": "/index.html",
  
      "exclude": ["/src/Assets/images/*.{png,jpg,gif}","/src/Assets/images/iconsMenu/*.{png,jpg,gif}", "/src/Assets/css/*", "/src/Assets/font/*"]
  
    }
  }